{
  "name": "pubmed-rag-assistant",
  "displayName": "PubMed RAG 智能助理",
  "version": "0.0.1",
  "description": "一个将大型语言模型与PubMed研究工作流深度融合的智能研究代理。",
  "author": "Your Name",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.11.0",
    "magic-string": "^0.30.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "plasmo": "^0.88.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5",
    "sharp": "^0.34.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*.pubmed.ncbi.nlm.nih.gov/*",
      "https://generativelanguage.googleapis.com/*",
      "<all_urls>"
    ],
    "permissions": [
      "storage",
      "tabs",
      "scripting",
      "side_panel"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "contents/scraper.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  },
  "pnpm": {
    "overrides": {
      "sharp": "^0.34.2"
    }
  }
}